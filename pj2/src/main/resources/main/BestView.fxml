<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.BestController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" alignment="CENTER" layoutX="150.0" layoutY="100.0" prefWidth="300.0">
        <Button fx:id="serializeButton" text="Serialize" onAction="#handleSerialize" />
        <Button fx:id="deserializeButton" text="Deserialize" onAction="#handleDeserialize" />
        <!-- Dodavanje TextArea za prikaz top vozila sa veÄ‡im prostorom za tekst -->
        <TextArea fx:id="topVehiclesTextArea" prefHeight="200.0" prefWidth="400.0" editable="false" wrapText="true"/>
    </VBox>
</AnchorPane>
